<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.takassoftware.managey">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme_OnTitle">
        <activity android:name=".InoutDetailActivity"></activity>
        <!-- android:theme="@style/Theme.AppCompat.Light.NoActionBar" -->
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme_NoneTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EntryActivity"
            android:theme="@style/AppTheme_NoneTitle" />
        <activity
            android:name=".MonthlyActivity"
            android:theme="@style/AppTheme_NoneTitle" />
        <activity
            android:name=".MonthlyDetailActivity"
            android:theme="@style/AppTheme_OnTitle" />
        <activity android:name=".InoutActivity" />
    </application>

</manifest>